{
    "name": "jamesrezo/webhelper",
    "description": "A Generic Httpd Configuration Helper",
    "homepage": "https://github.com/JamesRezo/WebHelper",
    "keywords": ["httpd", "configuration", "apache", "webserver"],
    "require": {
        "php": ">=5.4.0",
        "twig/twig": "~1.18",
        "composer/composer": "~1.0@dev",
        "symfony/console": "2.6.* || ~3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
   "autoload": {
        "psr-4": {
            "JamesRezo\\WebHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JamesRezo\\WebHelper\\Test\\": "tests/"
        }
    },
    "bin": ["bin/wh"],
    "license": "MIT",
